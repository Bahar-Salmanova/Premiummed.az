
@model ContactViewModel

<main>
    <section id="news2">
        <div class="div">
            <partial name="_breadcrumb" model="Model.BreadCrumb" />
        </div>
        <div class="container contact-container">
            <div class="row contact-row">
                <div class="col-lg-4 contact2">
                    <ul class="contact-list">
                        <li>
                            <span>ÜNVAN</span>
                            <p>@Model.Setting.Adress</p>
                        </li>
                        <li>
                            <span>MAIL ÜNVANI</span><br>
                            <a href="info@premiummed.az">@Model.Setting.Email</a>
                        </li>
                        <li>
                            <span>ƏLAQƏ NÖMRƏSİ</span>
                            <p>
                                @Model.Setting.Telephone <br>

                               @Model.Setting.Telephone2
                            </p>
                        </li>
                    </ul>
                </div>
                <div class="col-lg-8 map">
                    <div class="mapouter map-container">
                        <div class="gmap_canvas">
                            <iframe width="718" height="414" id="gmap_canvas"
                                    src="https://maps.google.com/maps?q=%20%C3%9Czeyir%20Hac%C4%B1b%C9%99yli%20k%C3%BC%C3%A7.,%2084&t=&z=13&ie=UTF8&iwloc=&output=embed"
                                    frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe><a href="https://yt2.org">youtube to mp3</a><br>
                            <style>
                                .mapouter {
                                    position: relative;
                                    text-align: right;
                                    height: 414px;
                                    width: 718px;
                                }
                            </style><a href="https://www.embedgooglemap.net">embedgooglemap.net</a>
                            <style>
                                .gmap_canvas {
                                    overflow: hidden;
                                    background: none !important;
                                    height: 353px;
                                    width: 718px;
                                }
                            </style>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </section>
</main>
