@model AboutViewModel


<main>
    <section id=news-main>
        <div class="container news-container">
            <img src="~/Uploads/testalize-me-0jE8ynV4mis-unsplash 1.jpg" alt="">
            <partial name="_breadcrumb" model="Model.BreadCrumb" />
        </div>
    </section>
    <section id="aboutus">
        <div class="container aboutus-container">
            <div class="row aboutus-row">
                <div class="col-lg-5 missions">
                    <ul class="list">
                        <li><a asp-controller="aboutus" asp-action="index" class="active">@ViewBag.TranslatedInfo["About"].Value</a> </li>
                        <li><a asp-controller="aboutus" asp-action="mission">@ViewBag.TranslatedInfo["Mission"].Value</a></li>
                        <li><a asp-controller="aboutus" asp-action="vision">@ViewBag.TranslatedInfo["Vision"].Value</a></li>
                        <li><a asp-controller="aboutus" asp-action="quality">@ViewBag.TranslatedInfo["Quality Policy"].Value</a></li>
                    </ul>
                </div>
                <div class="col-lg-7 description">
                    <p>
                        @ViewBag.TranslatedInfo["we who?"].Value
                    </p>
                </div>
            </div>
        </div>
    </section>
</main>

